	<title>@ViewBag.Title - My ASP.NET Application</title>
	<link href="~/Content/css/style.css" rel="stylesheet" />
	<link href="~/Content/css/animation.css" rel="stylesheet" />
	<script src="~/Scripts/jquery-1.10.2.min.js"></script>
	<script src="~/Scripts/app.js"></script>
	<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDBazgfqcZNAZoTKreU84uHXyMmBVVGtQE"></script>

</head>
<body>

	<header>
		<div class="wrapper dis-flex-between">
			<div class="logo">
				<a href="/Home/Index"><img class="@ViewBag.home" src="~/Content/images/logo/logo.png"></a>
			</div>
			<nav>
				<a href="/Dreams/Index"><img class="@ViewBag.dreamsicon" src="~/Content/images/nav/nav1.png"></a>
				<a href="/Dreams/Search"><img class="@ViewBag.search" src="~/Content/images/nav/nav2.png"></a>
				<a href="/Map/Index"><img class="@ViewBag.map" src="~/Content/images/nav/nav3.png"></a>
				@if (Session["User"] == null)
				{
					<a href="/Account/SignIn"><img class="@ViewBag.signin" src="~/Content/images/nav/nav4.png"></a>
				}
				else
				{
					<div class="dropdown">
						<div id="dropdownMenuButton">
							<img style="width:50px;" class="category_icon @ViewBag.login "  src="~/Content/SVG/PROFILEicon.svg">
						</div>

						<div class="drop" id="drop" style="display:none;">
							<a class="dropdown-item" href="/User/Dreams">My Dreams</a>
							<a class="dropdown-item" href="/Dreams/AddDreams">New Dream</a>
							<a class="dropdown-item" href="#" onclick="Logout()"> Wake up</a>
						</div>
					</div>
				}
			</nav>
		</div>

	</header>

	<div class="container body-content">
		@RenderBody()
	</div>

	<script>
		$(function () {
			$("nav img").each(function () {
				if ($(this).hasClass("active")) {
					var src = $(this).attr("src");
					var spl = src.split(".");
					$(this).attr("src", spl[0] + "-c." + spl[1])
				}
			})

			if ($(".logo img").hasClass("active")) {

				var src = $(".logo img").attr("src");
				var spl = src.split(".");
				$(".logo img").attr("src", spl[0] + "-c." + spl[1])
			}

			

			$('.dropdown').mouseover(function () {
				$('#drop').css("display", "block");
				$('#dropdownMenuButton > img').addClass('active');
				$('#dropdownMenuButton > img').removeClass('category_icon');
		
			})
			$('.dropdown').mouseout(function () {
				$('#drop').css("display", "none");
				$('#dropdownMenuButton > img').removeClass('active');
				$('#dropdownMenuButton > img').addClass('category_icon');
			})
		})
	</script>
</body>
</html>